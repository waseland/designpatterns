package tests;

import static org.junit.Assert.*;

import org.junit.Test;

import nodes.AndNode;

public class NodeTest {

	@Test
	public void test() {
		
		AndNode andNode = new AndNode();
		andNode.addValue(false);
		andNode.addValue(false);
		andNode.addValue(false);
		
		assertEquals("didWork() to return true", true, andNode.didWork());
	}

}
